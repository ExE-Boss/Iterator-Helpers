		"internal-slot": "^1.0.0",
		"object-inspect": "^1.6.0"
	},
	"devDependencies": {
		"@exe-boss/eslint-config": "^0.2.1",
		"@types/define-properties": "^1.1.0",
		"@types/globalthis": "^1.0.0",
